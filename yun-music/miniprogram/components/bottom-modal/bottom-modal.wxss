/* components/bottom-modal/bottom-modal.wxss */
.model{
  position: fixed;
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 99;/*层数*/
  text-align: center;
  background: rgba(0, 0, 0, 0.6);
}
.panel{
  position: absolute;
  bottom: 0rpx;
  left: 0;
  width: 100%;
  min-height: 300rpx;
  box-sizing: border-box;
  background-color: #f8f8f8;
}
/*直接修改外部样式*/
.model .icon-shanchuyitiao{
  position: absolute;
  right: 10rpx;
  top: 10rpx;
  padding:20rpx;/*增加点击区域大小*/
}
